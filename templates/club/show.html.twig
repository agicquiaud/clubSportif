{% extends 'base.html.twig' %}

{% block title %}Club{% endblock %}

{% block body %}
    <h1>Club</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ club.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ club.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ club.address }}</td>
            </tr>
            <tr>
                <th>PhoneNumber</th>
                <td>{{ club.phoneNumber }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ club.ville }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ club.zipCode }}</td>
            </tr>
            <tr>
                <th>Acronym</th>
                <td>{{ club.acronym }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('club_index') }}">back to list</a>

    <a href="{{ path('club_edit', {'id': club.id}) }}">edit</a>

    {{ include('club/_delete_form.html.twig') }}
{% endblock %}
